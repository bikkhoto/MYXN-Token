# Secure MYXN Token Deployment Environment Variables
# Copy this to .env and fill in your values
# NEVER commit .env to git!

# Network (devnet or mainnet-beta)
NETWORK=mainnet-beta
RPC_URL=https://api.mainnet-beta.solana.com

# Treasury Wallet - All funds move here
TREASURY_PUB=Azvjj21uXQzHbM9VHhyDfdbj14HD8Tef7ZuC1p7sEMk9

# Keypair Paths (NEVER COMMIT ACTUAL KEYPAIRS)
TMP_KEYPAIR_PATH=./keypairs/treasury-keypair.json

# Token configuration
TOTAL_SUPPLY=1000000000
DECIMALS=9

# Metadata file
METADATA_FILE=./metadata/secure-token-metadata.json

# Confirmation (set to 'true' to proceed with mainnet operations)
CONFIRM_MAINNET=false

# IPFS Configuration (for future metadata updates)
IPFS_PROVIDER=pinata
PINATA_JWT=your_pinata_jwt_here